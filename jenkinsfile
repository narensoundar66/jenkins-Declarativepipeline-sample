pipeline {
  agent any
  stages {
    stage('Preparation') 
    {
      steps{
         echo 'Successfully cloned repository...
      }
    }
    stage('Build') {
      steps{
          echo "changing into directory src"
      }
    }
    stage('Results') {
      steps{
        echo "Ending job ..."
      }
    }
  }
  post
  {
    fail{
      echo "build failed"
    }
  }
}
